{
  "name": "tsdice",
  "version": "1.0.0",
  "description": "tsDice - The Ultimate tsParticles Scene Randomizer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docs": "node scripts/build-docs.js",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint js/ tests/ --ext .js",
    "lint:fix": "eslint js/ tests/ --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,json,md,html,css}\"",
    "prepare": "husky"
  },
  "keywords": [
    "tsparticles",
    "particles",
    "animation",
    "randomizer"
  ],
  "author": "zophiezlan",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "happy-dom": "^20.3.3",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "marked": "^17.0.1",
    "prettier": "^3.7.4",
    "vite": "^7.3.1",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^4.0.16"
  }
}
